<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
    <script src="https://cdn.jsdelivr.net/npm/chart.js@2.8.0"></script>
    <style>
      body {
        background-color: antiquewhite;
      }
      .nav {
        background-color: azure;
        display: flex;
        padding: 0.5em;
        margin-top: 0px;
        font-size: 24px;
        font-family: 'Courier New', Courier, monospace;
        font-weight: bold;
        justify-content: space-around;
      }
      a {
        color: darkolivegreen;
      }
      a:hover {
        color: green;
      }
    </style>
  </head>
  <body>
    <div class="nav">
      <span>Coronaspridning.nu</span>
      <a href="/cases">Cases</a>
      <a href="/deaths">Deaths</a>
      <a href="/recoveries">Recoveries</a>
      <a href="/map">Map</a>
    </div>
    <canvas id="myChart"></canvas>
    <script>
      let list = <%-JSON.stringify(arrOfNewObjects)%>;
      var ctx = document.getElementById('myChart').getContext('2d');
       let datasets = [];
       list.map(row => {
        let a = {};
        a.label = row.country;
        a.borderColor = Math.floor(Math.random()*16777215).toString(16);
        a.data = row.timeSeries;
        // console.log(a);
        datasets.push(a);
       });
       console.log(datasets);
      var chart = new Chart(ctx, {
        // The type of chart we want to create
        type: 'line',
        // The data for our dataset
        data: {
          labels: list[17].timeSeries.map(function (a) { return a.x; }),

          datasets: [{
              label: JSON.stringify(list[17].country),
              // backgroundColor: 'rgb(255, 99, 132)',
              borderColor: 'rgb(255, 99, 132)',
              data: list[17].timeSeries
            },]
          // datasets
        },

        // Configuration options go here
        options: {}
      });
    </script>
  </body>
</html>
